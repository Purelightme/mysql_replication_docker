[mysqld]
character-set-server=utf8mb4

server-id=0
log-bin=mysql-bin
binlog_format=RAW
expire_logs_days=365
log_output=table
general_log=on

gtid_mode=on
enforce_gtid_consistency=1

innodb_flush_log_at_trx_commit=1
sync_binlog=1

[mysql]
default-character-set=utf8mb4

[client]
default-character-set=utf8mb4
